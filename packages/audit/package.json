{
  "name": "@ccbd-seo/audit",
  "version": "1.0.0",
  "description": "SEO site health auditing with meta, content, structure, and performance rules",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@ccbd-seo/core": "workspace:*",
    "@ccbd-seo/content-analysis": "workspace:*",
    "@ccbd-seo/readability": "workspace:*",
    "@ccbd-seo/schema": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "^6.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "seo",
    "audit",
    "site-health",
    "page-audit",
    "typescript"
  ],
  "sideEffects": false
}
